{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "moduleResolution": "Node",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true
  },
  "typedocOptions": {
    "entryPoints": [
      "src/script.ts",
      "src/classes/pf.ts",
      "src/classes/ball.ts",
      "src/classes/block.ts",
    ],
    "out": "docs"
  },
  "include": [
    "src"
  ]
}